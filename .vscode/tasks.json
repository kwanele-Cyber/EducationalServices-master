{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build Solution",
            "type": "shell",
            "command": "msbuild.exe",
            "args": [
                "${workspaceFolder}\\EducationalServices.sln",
                "/t:Rebuild",
                "/p:Configuration=Release"
            ],
            "group": "build",
            "presentation": {
                "reveal": "always"
            },
            "problemMatcher": "$msCompile"
        }
    ]
}